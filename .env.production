# Configurações de Produção - brandaocontador-nfe-frontend

# API Configuration (produção)
VITE_API_URL=https://api.brandaocontador.com.br
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME="NFe Brandão Contador"
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Sistema de emissão de Notas Fiscais Eletrônicas"

# Environment
VITE_BASE=/
VITE_NODE_ENV=production

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_API=false

# NFe Configuration
VITE_NFE_AMBIENTE_DEFAULT=homologacao
VITE_NFE_SERIE_DEFAULT=1
VITE_NFE_MODELO_DEFAULT=55

# Security
VITE_SESSION_TIMEOUT=3600000
VITE_TOKEN_REFRESH_THRESHOLD=300000

# UI Configuration
VITE_THEME_DEFAULT=light
VITE_LANGUAGE_DEFAULT=pt-BR
VITE_ITEMS_PER_PAGE=10

# Company Information
VITE_COMPANY_NAME="Brandão Contador"
VITE_COMPANY_WEBSITE=https://brandaocontador.com.br
VITE_COMPANY_PHONE="(11) 99999-9999"

# Email Configuration
VITE_EMAIL_SUPPORT=suporte@brandaocontador.com.br
VITE_EMAIL_COMERCIAL=comercial@brandaocontador.com.br